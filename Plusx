echo ""
echo -e "\033[1;32m‚Ä¢ \033[1;32m‡πÄ‡∏Ñ‡∏•‡πá‡∏î‡∏•‡∏±‡∏ö! \033[1;33m‡∏õ‡∏£‡∏±‡∏ö‡πÉ‡∏ä‡πâ‡∏ò‡∏µ‡∏°‡∏°‡∏∑‡∏î‡πÉ‡∏ô‡πÄ‡∏ó‡∏≠‡∏£‡πå‡∏°‡∏¥‡∏ô‡∏±‡∏•‡∏Ç‡∏≠‡∏á‡∏Ñ‡∏∏‡∏ì\033[0m"
echo -e "\033[1;33m  ‡∏õ‡∏£‡∏∞‡∏™‡∏ö‡∏Å‡∏≤‡∏£‡∏ì‡πå‡∏ó‡∏µ‡πà‡∏î‡∏µ‡∏Ç‡∏∂‡πâ‡∏ô‡πÅ‡∏•‡∏∞‡∏Å‡∏≤‡∏£‡∏°‡∏≠‡∏á‡πÄ‡∏´‡πá‡∏ô‡∏Ç‡∏≠‡∏á‡∏™‡∏¥‡πà‡∏á‡πÄ‡∏î‡∏µ‡∏¢‡∏ß‡∏Å‡∏±‡∏ô!\033[0m"
echo ""
echo -e "\033[1;31m‚â†√ó‚â†√ó‚â†√ó‚â†√ó‚â†√ó‚â†√ó‚â†√ó‚â†√ó[\033[1;33m ‚Ä¢ \033[1;32mBY Gun-X VPN\033[1;33m ‚Ä¢\033[1;31m ]‚â†√ó‚â†√ó‚â†√ó‚â†√ó‚â†√ó‚â†√ó‚â†√ó‚â†√ó\033[0m"
echo ""
read -p "$(echo -e "\033[1;36mInstall Script \033[1;31m? \033[1;33m[Y/N]:\033[1;37m ")" -e -i Y resp
[[ $resp = @(n|N) ]] && rm $HOME/Plusx && exit 0
_lnk2=$(echo "$key"|awk -F : '{print $2}'); _lnk1=$(echo "$key"|awk -F : '{print $1}')
echo -e "\n\033[1;36m‡∏Å‡∏≥‡∏•‡∏±‡∏á‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö... ‡∏£‡∏∞‡∏ö‡∏ö\033[1;37m File list\033[0m" ; rm $_Ink/list > /dev/null 2>&1; wget -P $_Ink https://raw.githubusercontent.com/LiLGun-X/HYPER-X-SCRIPT/main/INSTALL/list > /dev/null 2>&1; verif_key
echo -e "\n\033[1;32m‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à!\033[1;32m"
sleep 1s
echo ""
[[ -f "$HOME/userlist.gzn" ]] && {
    clear
system=$(cat /etc/issue.net)
date=$(date '+%Y-%m-%d <> %H:%M:%S')
echo -e "\033[1;37m $system                          $date"
echo ""
echo "       ‚ñë‚ñí‚ñà‚ñë‚ñí‚ñà‚ñë‚ñí‚ñà‚ñë‚ñë‚ñí‚ñà‚ñë‚ñí‚ñà‚ñÄ‚ñÄ‚ñà‚ñë‚ñí‚ñà‚ñÄ‚ñÄ‚ñÄ‚ñë‚ñí‚ñà‚ñÄ‚ñÄ‚ñÑ‚ñë‚ñë‚ñë‚ñë‚ñÄ‚ñÑ‚ñë‚ñÑ‚ñÄ"|lolcat
echo "       ‚ñë‚ñí‚ñà‚ñÄ‚ñÄ‚ñà‚ñë‚ñí‚ñÄ‚ñÑ‚ñÑ‚ñÑ‚ñÄ‚ñë‚ñí‚ñà‚ñÑ‚ñÑ‚ñà‚ñë‚ñí‚ñà‚ñÄ‚ñÄ‚ñÄ‚ñë‚ñí‚ñà‚ñÑ‚ñÑ‚ñÄ‚ñë‚ñÄ‚ñÄ‚ñë‚ñë‚ñí‚ñà‚ñë‚ñë"|lolcat
echo "       ‚ñë‚ñí‚ñà‚ñë‚ñí‚ñà‚ñë‚ñë‚ñë‚ñí‚ñà‚ñë‚ñë‚ñë‚ñí‚ñà‚ñë‚ñë‚ñë‚ñë‚ñí‚ñà‚ñÑ‚ñÑ‚ñÑ‚ñë‚ñí‚ñà‚ñë‚ñí‚ñà‚ñë‚ñë‚ñë‚ñë‚ñÑ‚ñÄ‚ñí‚ñÄ‚ñÑ"|lolcat
echo "                                         ùôéùòæùôçùôÑùôãùôè"
    echo -e "\n\033[0;34m‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê\033[0m"
    echo ""
	echo -e "                 \033[1;33m‚Ä¢ \033[1;31m‡∏Ñ‡∏≥‡πÄ‡∏ï‡∏∑‡∏≠‡∏ô! \033[1;33m‚Ä¢ \033[0m"
	echo ""
    echo -e "\033[1;33m‡∏ê‡∏≤‡∏ô‡∏Ç‡πâ‡∏≠‡∏°‡∏π‡∏•‡∏ú‡∏π‡πâ‡πÉ‡∏ä‡πâ \033[1;32m(userlist.gzn) \033[1;33m‡∏Ñ‡∏∑‡∏≠" 
    echo -e "‡∏°‡∏µ‡πÅ‡∏•‡πâ‡∏ß! ‡∏Ñ‡∏∏‡∏ì‡∏ï‡πâ‡∏≠‡∏á‡∏Å‡∏≤‡∏£‡∏ó‡∏µ‡πà‡∏à‡∏∞‡πÉ‡∏ä‡πâ‡∏Ç‡πâ‡∏≠‡∏°‡∏π‡∏•‡∏õ‡∏±‡∏à‡∏à‡∏∏‡∏ö‡∏±‡∏ô‡∏ô‡∏µ‡πâ ‡πÉ‡∏ä‡πà‡∏´‡∏£‡∏∑‡∏≠‡πÑ‡∏°‡πà?"
	echo -e "‡∏ö‡∏±‡∏ç‡∏ä‡∏µ‡∏ú‡∏π‡πâ‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô‡∏õ‡∏±‡∏à‡∏à‡∏∏‡∏ö‡∏±‡∏ô? ‡∏´‡∏£‡∏∑‡∏≠‡∏Ñ‡∏∏‡∏ì‡∏ï‡πâ‡∏≠‡∏á‡∏Å‡∏≤‡∏£"
    echo -e "‡∏™‡∏£‡πâ‡∏≤‡∏á‡∏ê‡∏≤‡∏ô‡∏Ç‡πâ‡∏≠‡∏°‡∏π‡∏•‡πÉ‡∏´‡∏°‡πà?\033[0m"
	echo -e "\n\033[1;37m[\033[1;31m1\033[1;37m] \033[1;33m‡πÉ‡∏ä‡πâ‡∏ê‡∏≤‡∏ô‡∏Ç‡πâ‡∏≠‡∏°‡∏π‡∏•‡∏õ‡∏±‡∏à‡∏à‡∏∏‡∏ö‡∏±‡∏ô‡∏ô‡∏µ‡πâ\033[0m"
	echo -e "\033[1;37m[\033[1;31m2\033[1;37m] \033[1;33m‡∏™‡∏£‡πâ‡∏≤‡∏á‡∏ê‡∏≤‡∏ô‡∏Ç‡πâ‡∏≠‡∏°‡∏π‡∏•‡πÉ‡∏´‡∏°‡πà\033[0m"
	echo -e "\n\033[0;34m‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê\033[0m"
